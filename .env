# GSY Data Extraction Environment Configuration
# =============================================

# API Authentication
# Get your API key from: https://ft.embraer.com.br/wiki/index.php?title=API_Key
API_KEY=

# Date Range Configuration
# Format: YYYY/MM/DD
# DATE_INI: Start date for data extraction
# DATE_END: End date (if empty, defaults to 3 months from today)
DATE_INI=2010/01/01
DATE_END=

# Environment Selection
# true = Use QAS (homologation) environment: https://ft-qas.embraer.com.br
# false = Use Production environment: https://ft.embraer.com.br
GET_DATA_FROM_QAS=true

# Debug Configuration
# true = Enable debug mode (creates debug files)
# false = Disable debug mode (production use)
DEBUG=false

# Network Configuration
# API_TIMEOUT: Request timeout in seconds (increase for slow networks)
# MAX_RETRIES: Number of retry attempts for failed requests
API_TIMEOUT=300
MAX_RETRIES=10
